        <id column="HEALTH_ID" property="healthId" />
        <result column="HEALTH_TIME" property="healthTime" />
        <result column="HEALTH_BYQTZID" property="healthByqtzid" />
        <result column="HEALTH_ALID" property="healthAlid" />
        <result column="HEALTH_WJLJ" property="healthWjlj" />
